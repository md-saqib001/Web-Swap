# Example environment variables for database configuration
# Place a .env file in the project root (do NOT commit it).

# DB_DIR: folder (relative to project root) where the DB file will live
# Default: 'database'
DB_DIR=database

# DB_PATH: full path to the DB file. If set, this overrides DB_DIR.
# Example (Windows): C:\\data\\prod-dbs\\internet-roulette.db
# Example (Unix): /var/lib/internet-roulette/database.db
#DB_PATH=

ADMIN_PASSWORD=
